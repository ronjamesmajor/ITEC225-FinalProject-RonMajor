<UserControl x:Class="ITEC225_FinalProject_RonMajor.DashboardEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ITEC225_FinalProject_RonMajor"
             mc:Ignorable="d" 
             d:DesignHeight="110" d:DesignWidth="800">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="100" Stroke="Black" VerticalAlignment="Center" Width="780" Fill="#FFF8F2F5"/>
        <Label x:Name="lblEntryTitle" Content="Entry Title" HorizontalAlignment="Left" Margin="80,0,0,0" VerticalAlignment="Center" Height="40" Width="140" FontSize="22"/>
        <Label x:Name="lblProcess" Content="Process" HorizontalAlignment="Left" VerticalAlignment="Center" Height="40" Width="155" FontSize="22" Margin="320,0,0,0"/>
        <Label x:Name="lblStatus" Content="Status" HorizontalAlignment="Left" Margin="640,0,0,0" VerticalAlignment="Center" Height="40" Width="155" FontSize="22"/>

    </Grid>
</UserControl>
